<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ks49team01.admin.mapper.AdminCloseSchoolComparisonBranchMapper">

	<resultMap type="AdminCloseSchool" id="closeSchoolResultMap">
		<id column="close_school_code" property="closeSchoolCode"/>
		<result column="total_map" 				property="totalMap"/>
		<result column="close_school_code" 		property="closeSchoolCode"/>
		<result column="member_id" 			property="memberId"/>
		<result column="branch_code" 		property="branchCode"/>
		<result column="SD_EDU_OFFC_DIV_NM" 		property="coordinate"/>
		<result column="change_date" 		property="changeDate"/>
		<result column="first_date" 		property="firstDate"/>
		<result column="current_status" 		property="currentStatus"/>
	</resultMap>
	
 	<select id="getCloseSchool" resultMap="closeSchoolResultMap">
 		SELECT
			cs.total_map,
			cs.close_school_code,
			cs.member_id,
			cs.branch_code,
			cs.SD_EDU_OFFC_DIV_NM,
			cs.change_date,
			cs.first_date,
			cs.current_status
		FROM
			close_school_comparison_branch AS cs;
 	</select>
</mapper>