<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ks49team01.admin.mapper.AdminBranchApplyMapper">

	<resultMap type="AdminBranchApply" id="applyResultMap">
		<id column="branch_apply_code" 						property="branchApplyCode"/>
		<result column="branch_recruit_code" 				property="branchRecruitCode"/>		
		<result column="close_school_code" 					property="closeSchoolCode"/>		
		<result column="member_id" 							property="memberId"/>		
		<result column="branch_apply_date" 					property="branchApplyDate"/>		
	</resultMap>
	
 	<select id="getAdminBranchApplyList" resultMap="applyResultMap">
		SELECT
			b.member_id,
			b.branch_apply_date,
			c.close_school_img,
			c.view_map_img
		FROM
			branch_apply AS b
			INNER JOIN
			close_school AS c
			ON
			b.close_school_code = c.close_school_code;
	
 	</select>
 	
 	
</mapper>